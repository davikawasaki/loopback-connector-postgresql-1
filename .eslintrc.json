{
    "env": {
        "browser": false,
        "commonjs": true,
        "es6": true,
        "node": true,
        "mocha": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-useless-escape": 0,
        "no-unused-vars": 0,
        "no-extra-semi": 0,
        "no-undef": 1
    }
}

